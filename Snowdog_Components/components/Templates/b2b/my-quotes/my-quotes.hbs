<div class="page-wrapper">
    {{ render '@header' }}
    <main
        id="maincontent"
        class="
            dashboard
            container
            {{ class }}
        "
    >
        {{ render '@breadcrumbs' breadcrumbs }}
        <div class="row">
            <div
                class="
                    col-xs-12
                    col-lg-3
                    {{ sidebarClass }}
                "
            >
                {{ render '@dashboard-nav' }}
            </div>
            <div
                class="
                    col-xs-12
                    col-lg-9
                    {{ contentClass }}
                "
            >
                {{ render '@heading' heading }}
                {{ render '@button' switchButton merge=true }}
                {{ render '@table' quotesTable }}
                {{ render '@toolbar--secondary' toolbar merge=true }}
            </div>
        </div>
    </main>
</div>
{{ render '@footer' }}
