<div class="page-wrapper">
    {{ render '@header' }}
    <div class="container auth">
        {{ render '@breadcrumbs' breadcrumbs }}
        {{ render '@heading' mainHeading merge=true }}
        <div
            class="
                auth__links
                auth__links--registration
                {{loginLinks.class}}
            "
        >
            <a
                href="#"
                class="
                    button
                    button--link
                    auth__link
                    {{ loginLinks.loginClass }}
                "
            >
                Login
                {{ render '@icon' linkIcon }}
            </a>
            <a
                href="#"
                class="
                    button
                    button--link
                    auth__link
                    auth__link--registration
                    {{ loginLinks.registerClass }}
                "
            >
                Create account
                {{ render '@icon' linkIcon }}
            </a>
        </div>
        {{ render '@account-form--registration' }}
    </div>
</div>
<h1>test</h1>
{{ render '@footer' }}
